{
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/frontend-react/index.html"
    }
  ],
  "builds": [
    {
      "src": "frontend-react/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "installCommand": "npm install --prefix frontend-react && npm install --prefix backend"
}